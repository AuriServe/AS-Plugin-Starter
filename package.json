{
  "name": "as-plugin-starter",
  "version": "1.0.0",
  "description": "Starter for a Typescript AuriServe plugin.",
  "main": "index.js",
  "author": "Auri Collings (Aurailus)",
  "license": "MIT",
  "keywords": [
    "preact",
    "react",
    "typescript",
    "boilerplate"
  ],
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "build": "webpack --mode=production --progress --config ./webpack.ts",
    "start": "webpack --mode=development --watch --progress --config ./webpack.ts"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/transform.js"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@types/jest": "^26.0.10",
    "@types/webpack": "^4.41.21",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "eslint-plugin-jsdoc": "^30.3.0",
    "babel-loader": "^8.1.0",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^4.0.2",
    "html-webpack-plugin": "^4.4.1",
    "jest": "^26.4.2",
    "ts-loader": "^8.0.3",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "preact": "^10.4.8",
    "tslib": "^2.0.1"
  }
}
